{
	"name" : "openworks-srl/documentor",
	"description" : "Generatore di documenti a partire da template twig",
	"type" : "library",
	"require" : {
		"php": "5.4 - 5.6",
		"twig/twig" : "1.42.3",
		"symfony/yaml" : "3.3.18",
		"phpoffice/phpword" : "^0.17.0",
		"phpoffice/phpspreadsheet" : "1.8.2",
		"mikehaertl/phpwkhtmltopdf" : "^2.4",
		"doctrine/instantiator" : "1.0.5",
		"northwoods/config" : "1.1.0",
		"josegonzalez/dotenv": "dev-master"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5",
		"jms/serializer" : "1.7.*",
		"phpdocumentor/phpdocumentor" : "2.9.0"
		
	},
	"license" : "Apache-2.0",
	"authors" : [{
			"name" : "Mattia Bonzi",
			"email" : "mattiabonzi@openworks.it"
		}
